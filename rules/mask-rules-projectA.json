{
  "version": 1,
  "rules": [
    {
      "id": "tenant_id",
      "name": "Tenant ID",
      "enabled": true,
      "pattern": "\\btenantId=([A-Za-z0-9._-]{2,})\\b",
      "replacement": "tenantId=[MASKED_TENANT_ID]",
      "flags": []
    },
    {
      "id": "employee_no",
      "name": "Employee No",
      "enabled": true,
      "pattern": "\\bemployeeNo=([A-Za-z0-9._-]{2,})\\b",
      "replacement": "employeeNo=[MASKED_EMPLOYEE_NO]",
      "flags": []
    },
    {
      "id": "account_id",
      "name": "Account ID",
      "enabled": true,
      "pattern": "\\baccountId=([A-Za-z0-9._-]{4,})\\b",
      "replacement": "accountId=[MASKED_ACCOUNT_ID]",
      "flags": []
    },
    {
      "id": "internal_host",
      "name": "Internal Host",
      "enabled": true,
      "pattern": "\\bhost=(svc-internal-[0-9]{2})\\b",
      "replacement": "host=[MASKED_HOST]",
      "flags": []
    }
  ]
}
